{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext",
    ],
    "module": "ESNext",
    "rootDir": "src",
    "outDir": "dist",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "allowJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true
  }
}